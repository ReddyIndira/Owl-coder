#User function Template for python3


class Solution:
    def MissingNumber(self,array,n):
        '''k=[]
        for i in range(1,n+1):
            k.append(i)
            if i not in array:
                return i
                break'''
        k=sum(array)
        t=(n*(n+1))//2
        return(t-k)
        
        
